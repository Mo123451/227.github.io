{
  "version": 2,
  "functions": {
    "api/margin-trading.py": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/stock/margin-trading(.*)",
      "dest": "/api/margin-trading.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "(.*)",
      "dest": "/index.html"
    }
  ]
}